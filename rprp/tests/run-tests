#!/bin/bash

s='^mirp:|pass|FAIL'
s='pass|FAIL'

BIN="../mirprp.js"

echo -n 'mirp_biggo: '
node $BIN data < ../data/mirp_biggo.json | grep -P "$s"

echo -n 'mirp_err: '
node $BIN data < ../data/mirp_err.json | grep -P "$s"

echo -n 'mirp_fail: '
node $BIN data < ../data/mirp_fail.json | grep -P "$s"

echo -n 'mirp_fail1: '
node $BIN data < ../data/mirp_fail1.json | grep -P "$s"

echo -n 'mirp_fail2: '
node $BIN data < ../data/mirp_fail2.json | grep -P "$s"

echo -n 'mirp_fail3: '
node $BIN data < ../data/mirp_fail3.json | grep -P "$s"

echo -n 'mirp_L: '
node $BIN data < ../data/mirp_L.json | grep -P "$s"

echo -n 'mirp_nonmin: '
node $BIN data < ../data/mirp_nonmin.json | grep -P "$s"

echo -n 'mirp_pinwheel: '
node $BIN data < ../data/mirp_pinwheel.json | grep -P "$s"

echo -n 'mirp_x: '
node $BIN data < ../data/mirp_x.json | grep -P "$s"

echo -n 'mirp_z: '
node $BIN data < ../data/mirp_z.json | grep -P "$s"

